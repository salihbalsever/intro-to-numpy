from numpy import random
import numpy as np


# random.shuffle() shuffles the elements of an array in-place, modifying the original array.
arr = np.array([1, 2, 3, 4, 5])
random.shuffle(arr)

print(arr)

#NumPyâ€™s random.permutation() returns a randomly permuted sequence or array without modifying the original.

arr = np.array([1, 2, 3, 4, 5])
perm = random.permutation(arr)

print(perm)